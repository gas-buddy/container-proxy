{
  "name": "@gasbuddy/container-proxy",
  "version": "1.0.0",
  "description": "A reverse proxy that allows dynamic endpoint registration",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src/ -Dd build/",
    "lint": "eslint .",
    "start": "node build/server.js",
    "start-dev": "node -r babel-register src/server.js",
    "build-docker": "docker build -t gasbuddy/container-proxy .",
    "publish-docker": "docker push gasbuddy/container-proxy"
  },
  "keywords": [
    "proxy",
    "docker",
    "microservice",
    "gasbuddy"
  ],
  "author": "Developers <developers@gasbuddy.com>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-object-entries": "^1.0.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "pretty-data": "^0.40.0",
    "redbird": "^0.6.15",
    "window-size": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-gasbuddy": "^1.0.1",
    "babel-register": "^6.16.3",
    "cross-env": "^3.1.3",
    "eslint": "^3.7.0",
    "eslint-config-gasbuddy": "^1.0.0"
  }
}
