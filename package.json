{
  "name": "@gasbuddy/container-proxy",
  "version": "3.0.0",
  "description": "A reverse proxy that allows dynamic endpoint registration",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src/ -Dd build/",
    "lint": "eslint .",
    "start": "node build/server.js",
    "start-dev": "node -r @babel/register src/server.js",
    "build-docker": "docker build --squash -t gasbuddy/container-proxy .",
    "publish-docker": "docker push gasbuddy/container-proxy"
  },
  "engines": {
    "node": ">8.9"
  },
  "keywords": [
    "proxy",
    "docker",
    "microservice",
    "gasbuddy"
  ],
  "author": "Developers <developers@gasbuddy.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.0",
    "pretty-data": "^0.40.0",
    "redbird": "github:OptimalBits/redbird#ce7c5fad30a220c1a476a75f92f8423e74380f05",
    "window-size": "^1.1.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-preset-gasbuddy": "^5.5.0",
    "cross-env": "^5.2.0",
    "eslint-config-gasbuddy": "^5.2.1"
  }
}
